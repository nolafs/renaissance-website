{
  "name": "lol-hugo-webpack",
  "version": "1.0.0",
  "description": "League of Legend theme",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "cross-env NODE_ENV=development hugo -d ../dist -s site -vw",
    "start:webpack": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist && rimraf deploy",
    "afterbuild": "rimraf deploy/categories && rimraf deploy/modules && rimraf deploy/profiles && rimraf deploy/tags && rimraf deploy/profiles && rimraf deploy/sitemap.xml  && rimraf deploy/index.xml && rimraf deploy/index.html",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "cross-env NODE_ENV=production hugo -d ../deploy -s site -v --minify && npm run afterbuild",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "staging": "npm run staging:webpack && npm run staging:hugo",
    "staging:preview": "npm run staging:webpack && npm run build:hugo:preview",
    "staging:hugo": "cross-env NODE_ENV=staging  hugo -d ../dist -s site -v",
    "staging:hugo:preview": "npm run build:hugo -- -D -F",
    "staging:webpack": "cross-env NODE_ENV=staging ./node_modules/.bin/webpack --config webpack.config.stage.js",
    "staging:rsync": "npm run staging && rsync -zrSlh -e 'ssh -p 1001' -og --chown=apache:webmaster --stats --exclude-from=deployment-exclude-list.txt dist/*  root@biffapps.com:/home/websites/lol/public_html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-import": "^2.14.0",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^4.0.0",
    "foundation-sites": "^6.5.3",
    "hugo-bin": "^0.37.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "plugin-error": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.4.1"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
